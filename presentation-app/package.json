{
  "name": "presentation-framework",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx scripts/check-tts-cache.ts && tsx scripts/calculate-durations.ts && vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "eslint --ext .ts,.tsx src --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "tts:check": "tsx scripts/check-tts-cache.ts",
    "tts:generate": "tsx scripts/generate-tts.ts",
    "tts:from-json": "tsx scripts/generate-tts.ts --from-json",
    "tts:duration": "tsx scripts/calculate-durations.ts",
    "extract-narration": "tsx scripts/extract-narration.ts",
    "check-narration": "tsx scripts/check-narration.ts",
    "narration-api": "node server/narration-api.cjs",
    "dev:full": "concurrently \"npm run narration-api\" \"npm run dev\" --names \"API,VITE\" --prefix-colors \"cyan,magenta\"",
    "tts:align": "tsx scripts/generate-alignment.ts",
    "tts:verify": "tsx scripts/verify-tts.ts",
    "test:overflow": "tsx scripts/run-overflow-test.ts",
    "test:screenshot": "tsx scripts/run-screenshot-test.ts",
    "test:record": "tsx scripts/run-record-test.ts",
    "record:obs": "tsx scripts/record-obs.ts",
    "dead-code": "knip --include exports"
  },
  "dependencies": {
    "@xyflow/react": "^12.10.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer-motion": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-rough-notation": "^1.0.8",
    "shiki": "^3.23.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^24.8.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^5.0.4",
    "axios": "^1.12.2",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "get-audio-duration": "^4.0.1",
    "jsdom": "^28.1.0",
    "knip": "^5.85.0",
    "obs-websocket-js": "^5.0.7",
    "prettier": "^3.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
